{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "dependencyDashboard": true,
  "semanticCommitType": "fix",
  "packageRules": [
    {
      "matchUpdateTypes": ["patch"],
      "groupName": "patch updates"
    },
    {
      "matchUpdateTypes": ["minor"],
      "groupName": "minor updates"
    }
  ]
}
